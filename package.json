{
  "name": "redux-place",
  "version": "0.0.0",
  "description": "Learning project for the purpose of learning React, Redux with Typescript and Jest testing",
  "main": "webpack.config.js",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --inline --hot --content-base ./build --historyApiFallback true",
    "build": "webpack",
    "test": "jest --no-cache",
    "test:watch": "jest --watch",
    "test:debug": "jest --no-cache",
    "test:updateSnapshot": "jest --no-cache --updateSnapshot"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "./node_modules/ts-jest/preprocessor.js"
    },
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/src/fixtures/style-mock.ts"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js"
  },
  "dependencies": {
    "@types/react-router-dom": "^4.0.7",
    "html-webpack-plugin": "^2.28.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^4.4.5",
    "react-router-dom": "^4.2.2",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-promise-middleware": "^3.2.0",
    "redux-thunk": "^2.1.0",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5"
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.0",
    "@types/history": "^4.6.0",
    "@types/jest": "^19.2.2",
    "@types/node": "^7.0.39",
    "@types/react": "^15.0.23",
    "@types/react-dom": "^15.5.0",
    "@types/react-redux": "^4.4.47",
    "@types/react-router": "^4.0.15",
    "awesome-typescript-loader": "^3.1.3",
    "css-loader": "^0.28.4",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "jest": "^19.0.2",
    "node-sass": "^4.5.3",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "sass-loader": "^6.0.5",
    "sinon": "^2.4.1",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "ts-jest": "^19.0.14",
    "ts-loader": "^2.0.3",
    "typescript": "^2.4.0",
    "typings-for-css-modules-loader": "^1.5.0"
  },
  "author": "",
  "license": "ISC"
}
